<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<LangVersion>9.0</LangVersion>
		<Version>6.0.0</Version>
		<InformationalVersion>6.0.0</InformationalVersion>
		<ProductName>BlitzkriegSoftware.Data.Maker</ProductName>
		<Copyright>(c) 2000-2022 Blitzkrieg Software/Stuart Williams</Copyright>
		<StartupObject>BlitzkriegSoftware.Data.Maker.Program</StartupObject>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<DocumentationFile>.\BlitzkriegSoftware.Data.Maker.xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <WarningLevel>5</WarningLevel>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <WarningLevel>5</WarningLevel>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="CommandLineParser" Version="2.9.1" />
		<PackageReference Include="Faker.Data" Version="2.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	</ItemGroup>

</Project>
